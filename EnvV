import java.util.Map;

public class EnvPrinter {

    public static void printOsEnvVars() {
        Map<String, String> env = System.getenv();

        for (Map.Entry<String, String> entry : env.entrySet()) {
            System.out.println(
                entry.getKey() + " = " + entry.getValue());
        }
    }
}
